  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod "FilesProvider"
  pod 'SwiftSoup'
  # Pods for macB

  target 'macBTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'macBUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
